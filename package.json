{
  "name": "vue-material-admin-full",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "background": "nohup yarn start >/dev/null 2>&1 &",
    "dev": "vue-cli-service serve --open",
    "format": "prettier --write src/**/*.{vue,js}",
    "eslint": "eslint 'src/**/*.{js,vue}' --fix",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'",
    "now-start": "vue-cli-service serve",
    "start": "vue-cli-service serve",
    "pm2": "pm2 -i 4 start yarn --interpreter bash --name Lotto-Admin-Prod -- serve:prod",
    "start:prod": "vue-cli-service serve --mode production",
    "build:prod": "vue-cli-service build --mode production --target app --modern --dest tempdist && rm -rf dist && mv tempdist dist",
    "serve:prod": "serve -l 8082 -s dist"
  },
  "dependencies": {
    "@amcharts/amcharts4": "^4.10.5",
    "@amcharts/amcharts4-geodata": "^4.1.17",
    "apexcharts": "^3.22.0",
    "axios": "^0.20.0",
    "core-js": "^3.6.5",
    "cross-env": "^7.0.2",
    "css-loader": "^1.0.1",
    "font-awesome": "^4.7.0",
    "highcharts": "^8.2.0",
    "highcharts-vue": "^1.3.5",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "vue": "^2.6.12",
    "vue-apexcharts": "^1.6.0",
    "vue-router": "^3.4.6",
    "vue2-google-maps": "^0.10.7",
    "vuetify": "^2.3.15",
    "vuex": "^3.5.1",

    "@sweetalert2/theme-wordpress-admin": "^3.1.4",
    "ag-grid-community": "^23.0.2",
    "ag-grid-vue": "^23.0.2",
    "crypto": "^1.0.1",
    "is_js": "^0.9.0",
    "lodash": "^4.17.15",
    "numeral": "^2.0.6",
    "register-service-worker": "^1.6.2",
    "socket.io-client": "^2.3.0",
    "sprintf-js": "^1.1.2",
    "string-mask": "^0.3.0",
    "sweetalert2": "^9.10.10",
    "toastr": "^2.1.4",
    "vue-flag-icon": "^1.0.6",
    "vue-i18n": "^8.15.3",
    "vue-lazyload": "^1.3.3",
    "vue-page-transition": "^0.2.2",
    "vue-property-decorator": "^8.4.0",
    "vue-the-mask": "^0.11.1",
    "vuex-pathify": "^1.4.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-vuetify": "~2.0.7",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "vuetify-loader": "^1.3.0",
    "@vue/cli-plugin-pwa": "^4.3.1",
    "@vue/cli-plugin-router": "~4.3.1",
    "@vue/cli-plugin-vuex": "~4.3.1",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "1.0.0-beta.33",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^25.3.0",
    "eslint-plugin-prettier": "^3.1.2",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.4",
    "vee-validate": "^3.2.5",
    "vue-chartist": "^2.2.1",
    "vue-cli-plugin-i18n": "^0.6.0",
    "vue-world-map": "^0.1.1"

  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
